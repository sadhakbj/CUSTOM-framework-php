{
  "name":        "bijaya.kuikel/symfony",
  "authors":     [
    {
      "name":  "Bijaya Prasad Kuikel",
      "email": "sadhakbj@gmail.com"
    }
  ],
  "require":     {
    "symfony/http-foundation": "^6.1",
    "symfony/routing":         "^6.1",
    "symfony/http-kernel":     "^6.1",
    "symfony/error-handler":   "^6.1"
  },
  "require-dev": {
    "symfony/var-dumper": "^6.1",
    "phpunit/phpunit":    "^9.5"
  },
  "autoload":    {
    "psr-4": {
      "App\\":   "src/",
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "phpunit-cov": "XDEBUG_MODE=coverage ./vendor/bin/phpunit --coverage-text",
    "phpunit-cov-html": "XDEBUG_MODE=coverage ./vendor/bin/phpunit --coverage-html"
  }
}
